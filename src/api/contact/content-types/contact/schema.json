{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "regex": "^[A-Za-z.-]+(\\s*[A-Za-z.-]+)*$",
      "unique": false
    },
    "email": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "email",
      "required": true,
      "unique": false
    },
    "message": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    }
  }
}
