{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "User_Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "first_name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "last_name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": false
    },
    "Chooese_groom_bride": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Groom",
        "Bride"
      ],
      "required": true,
      "default": "Groom"
    },
    "Choose_veg_nonveg": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Vegetarian",
        "Non-Vegetarian"
      ],
      "required": true
    },
    "date_of_birth": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date",
      "required": true,
      "unique": false
    },
    "star": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Capricorn",
        "Aquarius",
        "Pisces",
        "Aries",
        "Taurus",
        "Gemini",
        "Cancer",
        "Leo",
        "Virgo",
        "Libra",
        "Scorpio",
        "Ophiuchus",
        "Sagittarius"
      ],
      "required": true
    },
    "Height": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal",
      "default": 0,
      "required": true,
      "unique": false
    },
    "Color": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "educational_qualification": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "minLength": 20
    },
    "career_detail": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "Salary_monthly_income": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "family_property_details": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "Expection": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "phone_number": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "required": true,
      "min": "10"
    },
    "caste": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "marriage_status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Single",
        "Married",
        "Widowed",
        "Divorced ",
        "Separated",
        "Registerd-Partnership"
      ],
      "required": true
    },
    "profile_photo": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "parent_informations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::parent-information.parent-information",
      "mappedBy": "user_profiles"
    },
    "family_members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::family-member.family-member",
      "mappedBy": "user_profiles"
    },
    "horoscope_informations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::horoscope-information.horoscope-information",
      "mappedBy": "user_profiles"
    }
  }
}
